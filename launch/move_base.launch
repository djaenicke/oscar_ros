<launch>
 <!-- Run the map server --> 
  <node name="map_server" pkg="map_server" type="map_server" args="$(find robo_car_ros_if)/maps/apt_map.yaml"/>

 <!--- Run AMCL --> 
  <include file="$(find amcl)/examples/amcl_diff.launch" />

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find robo_car_ros_if)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find robo_car_ros_if)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find robo_car_ros_if)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find robo_car_ros_if)/config/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find robo_car_ros_if)/config/local_planner_params.yaml" command="load" />
  </node>
</launch> 